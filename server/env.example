# Paystack Configuration
# Get these from your Paystack dashboard: https://dashboard.paystack.com/#/settings/developers

# Test Environment (for development)
PAYSTACK_PUBLIC_KEY_TEST=pk_test_your_test_public_key_here
PAYSTACK_SECRET_KEY_TEST=sk_test_your_test_secret_key_here

# Live Environment (for production)
PAYSTACK_PUBLIC_KEY_LIVE=pk_live_your_live_public_key_here
PAYSTACK_SECRET_KEY_LIVE=sk_live_your_live_secret_key_here

# Use the appropriate secret key based on environment
PAYSTACK_SECRET_KEY=sk_test_your_test_secret_key_here

# Frontend URL for callback
FRONTEND_URL=http://localhost:5173

# Other existing environment variables
MONGO_URI_PRODUCTION=your_mongodb_connection_string
JWT_SECRET=your_jwt_secret_key
NODE_ENV=development
PORT=5000
